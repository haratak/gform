{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*",
      "headers": [
        {"key": "X-Content-Type-Options", "value": "nosniff"},
        {"key": "X-Frame-Options", "value": "ALLOW-FROM https://get.google.com"},
        {"key": "X-UA-Compatible", "value": "ie=edge"},
        {"key": "X-XSS-Protection", "value": "1; mode=block"}
      ]
    }]
   
  }
}
